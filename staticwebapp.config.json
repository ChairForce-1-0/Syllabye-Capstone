{
    "globalHeaders": {
      "Cross-Origin-Opener-Policy": "same-origin",
      "Cross-Origin-Embedder-Policy": "require-corp"
    },
    "routes": [
      {
        "route": "/assets/*",
        "allowedRoles": ["anonymous"]
      },
      {
        "route": "/images/*",
        "allowedRoles": ["anonymous"]
      },
      {
        "route": "/*",
        "rewrite": "/index.html"
      }
    ]
  }
  